{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ES2022",
      "rootDir": "./src",
      "outDir": "./dist",
      "removeComments": true,
      "moduleResolution": "Bundler",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "strictNullChecks": true,
      "skipLibCheck": true,
    }
  }
  
  